<Window x:Class="Example.ModuleInject.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical">
        <TextBlock>Hello ModuleInject</TextBlock>
        <TextBlock>Here is an injected value:</TextBlock>
        <TextBlock Text="{Binding Path=Greeting}"></TextBlock>
    </StackPanel>
</Window>
